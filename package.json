{
  "name": "nacsos-web",
  "version": "0.20.2",
  "engines": {
    "npm": "^10.9.2",
    "node": "^v22.16.0"
  },
  "private": true,
  "config": {
    "nacsos_core": "http://127.0.0.1:8081",
    "nacsos_pipe": "http://127.0.0.1:8000"
  },
  "type": "module",
  "scripts": {
    "serve": "vite --host 127.0.0.1 --port 8080",
    "preview": "vite preview --host 127.0.0.1 --port 8080",
    "build-only": "vite build --outDir dist",
    "build": "run-p type-check build-only",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint .",
    "test:unit": "vitest --environment jsdom --root src/",
    "nql": "nearleyc src/util/nql/grammar.ne --out src/util/nql/grammar.ts",
    "api": "npx tsx tools/openapi-ts/generate.ts"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vueform/multiselect": "^2.6.11",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^13.4.0",
    "axios": "^1.10.0",
    "bootstrap": "^5.3.7",
    "bootstrap-vue-next": "^0.30.3",
    "chart.js": "^4.5.0",
    "core-js": "^3.43.0",
    "d3-array": "^3.2.4",
    "d3-axis": "^3.0.0",
    "d3-brush": "^3.0.0",
    "d3-color-legend": "^1.4.3",
    "d3-format": "^3.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-zoom": "^3.0.0",
    "diff": "^8.0.2",
    "form-data": "^4.0.3",
    "marked": "^15.0.12",
    "nearley": "^2.20.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^14.0.2",
    "@rushstack/eslint-patch": "^1.11.0",
    "@types/cross-spawn": "^6.0.6",
    "@types/d3": "^7.4.3",
    "@types/d3-scale": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/nearley": "^2.11.5",
    "@types/node": "^24.0.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/language-server": "^2.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "c12": "^3.0.4",
    "camelcase": "^8.0.0",
    "commander": "^14.0.0",
    "cross-spawn": "^7.0.6",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "fs-extra": "^11.3.0",
    "handlebars": "^4.7.8",
    "jsdom": "^26.1.0",
    "npm-check-updates": "^18.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  }
}
