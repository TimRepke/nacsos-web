{
  "name": "nacsos-web",
  "version": "0.18.0",
  "private": true,
  "config": {
    "nacsos_core": "http://127.0.0.1:8081",
    "nacsos_pipe": "http://127.0.0.1:8000"
  },
  "type": "module",
  "scripts": {
    "serve": "vite --host 127.0.0.1 --port 8080",
    "preview": "vite preview --host 127.0.0.1 --port 8080",
    "build-only": "vite build --outDir dist",
    "build": "run-p type-check build-only",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "test:unit": "vitest --environment jsdom --root src/",
    "nql": "nearleyc src/util/nql/grammar.ne --out src/util/nql/grammar.ts",
    "api": "npx tsx tools/openapi-ts/generate.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@vueform/multiselect": "^2.6.10",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.25.5",
    "chart.js": "^4.4.5",
    "core-js": "^3.38.1",
    "d3-array": "^3.2.4",
    "d3-axis": "^3.0.0",
    "d3-brush": "^3.0.0",
    "d3-color-legend": "^1.4.1",
    "d3-format": "^3.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-zoom": "^3.0.0",
    "form-data": "^4.0.1",
    "marked": "^14.1.3",
    "nearley": "^2.20.1",
    "pinia": "^2.2.4",
    "sass": "^1.80.1",
    "vue": "^3.5.12",
    "vue-chartjs": "^5.3.1",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "11.7.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/cross-spawn": "^6.0.6",
    "@types/jsdom": "^21.1.7",
    "@types/nearley": "^2.11.5",
    "@types/node": "^22.7.6",
    "@types/d3": "^7.4.3",
    "@types/d3-scale": "^4.0.8",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "c12": "2.0.1",
    "camelcase": "^8.0.0",
    "commander": "^12.1.0",
    "cross-spawn": "7.0.3",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.29.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "jsdom": "^25.0.1",
    "json-schema-ref-parser": "^9.0.9",
    "npm-check-updates": "^17.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  }
}
